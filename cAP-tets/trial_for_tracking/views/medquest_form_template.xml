  <template id="frontend_layout" name="Header extended" inherit_id="portal.frontend_layout">
    <xpath expr="//div[@id='oe_structure_header_default_1']" position="after">
      <button type="button" class="btn btn-danger ml-4" data-toggle="modal" data-target=".medquest_main_cl" style="width:100px;">Get Started</button>


      <button type="button" class="btn btn-danger ml-4" data-toggle="modal" data-target="#exampleModalScrollable" style="width:148px;">MEDQuEST Scroll</button>
      
